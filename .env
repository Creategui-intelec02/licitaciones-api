# ========================================
# CONFIGURACIÓN - Renombrar a .env
# ========================================

# Puerto en el que escucha el servidor
PORT=3000

# ⭐ IMPORTANTE: URL DE TU N8N WEBHOOK
# Reemplazar con tu URL real de n8n
# Ej: https://n8n.tudominio.com/webhook/0f78e5aa-5a79-43cc-8361-7d7148b1e8d4
N8N_WEBHOOK_URL=https://intelec01.app.n8n.cloud/webhook/licitaciones

# Tamaño máximo de archivo en MB
MAX_FILE_SIZE_MB=80

# Directorio temporal para archivos
TEMP_DIR=./uploads

# Environment (development, production)
NODE_ENV=development

